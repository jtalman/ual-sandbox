double p0      = sqrt(e0*e0-m0*m0);                       // derived beam momentum
double e       = e0+p0*p[5];
double gO      = e/m0;                                    // Outside

//     p[0]    = p[0];                                    // unchanged?
//     p[1]    = p[1];                                    // unchanged?
//     p[2]    = p[2];                                    // unchanged?
